#Compiler
CC=gcc

#Compilation flags
CFLAGS=-Werror

#Dependiencies for link list
DEPS_LL = linked_list.h

#Object files need it
OBJ = main.o linked_list.o

%.o: %.c $(DEPS_LL)
	$(CC) -c -o $@ $< $(CFLAGS)

llmain: $(OBJ)
	$(CC) -o $@ $^ $(CFLAGS)

clean:
	-rm *.o llmain	

	
